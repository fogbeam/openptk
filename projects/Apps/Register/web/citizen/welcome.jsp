<!-- welcome.jsp -->

<form name="welcome" action="">
   <input type=hidden name="mode" value="data"/>
   <table class="content">
      <tr>
         <td>
            <h3>Welcome</h3>
            <p>
               To successfully complete this process, you <b>MUST</b> provide:
            </p>
            <table>
               <tr valign="top">
                  <td align="right"><b>Personal Data:</b></td>
                  <td>
                     <ul>
                        <li><b>First Name</b></li>
                        <li><b>Last Name</b></li>
                        <li><b>Email Address</b></li>
                        <li><b>Middle Name</b></li>
                        <li><b>Last 4 SSN</b></li>
                        <li><b>Date Of Birth</b></li>
                        <li><b>Drivers License</b></li>
                        <li><b>Gender</b></li>
                     </ul>
                  </td>
                  <td align="right"><b>Home:</b></td>
                  <td>
                     <ul>
                        <li><b>Address One</b></li>
                        <li><b>City</b></li>
                        <li><b>State</b></li>
                        <li><b>Zip Code</b></li>
                        <li><b>County</b></li>
                        <li><b>Country</b></li>
                        <li><b>Email</b></li>
                        <li><b>Phone</b></li>
                     </ul>
                  </td>
                  <td align="right"><b>Contact:</b></td>
                  <td>
                     <ul>
                        <li><b>Email Addresses</b></li>
                        <li><b>Phone Numbers</b></li>
                     </ul>
                  </td>
               </tr>
            </table>

            <p>
               Click the <b>Next</b> button to begin.
            </p>
         </td>
      </tr>
   </table>
   <table class="buttons">
      <tr>
         <td>
            <input type="submit" value="Next"/>
         </td>
      </tr>
   </table>
</form>
