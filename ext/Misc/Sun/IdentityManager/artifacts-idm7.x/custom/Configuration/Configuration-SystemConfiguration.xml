    <!-- serverSettings is the root of a set of per-server configuration
         settings (intended to replace Waveset.properties in many cases).
         
         Each child of serverSettings defines the settings for a single
         server, with the name indicating the server (as returned by
         Util.getServerId()).  The special server "default" is used to
         set global defaults for settings; the values set in "default" will
         be used for a server unless they are specifically set in that
         server's configuration.
    -->
    <Attribute name='serverSettings'>
      <Object>
        <Attribute name='default'>
          <Object>
            <Attribute name='reconciler'>
              <Object>
                <!-- # of threads always kept alive for requests -->
                <Attribute name='minConcurrentResources' value='1'/>
                <!-- Maximum # of resources processed concurrently -->
                <Attribute name='maxConcurrentResources' value='3'/>
                <!-- # of threads always kept alive for recon processing -->
                <Attribute name='minWorkerThreads'/>
                <!-- Maximum # of threads to use for recon processing -->
		            <Attribute name='maxWorkerThreads'/>
                <!-- Minimum wait (in ms) between updates of reconcile
                     result data.  This affects how up-to-date the
                     ReconcileStatus view will be during a running
                     reconciliation.
                -->
                <Attribute name='resultThrottle' value='15000'/>
                <!-- Minimum wait (in ms) between updates of reconciler
                     statistics. This affects how up-to-date reconciler
                     internal state will be in the debug pages.
                  -->
                <Attribute name='statsThrottle' value='15000'/>
                <!-- Maximum number of times to retry failed
					 reconcile requests.
                  -->
                <Attribute name='maxRetries' value='3'/>
                <!-- Minimum wait (in ms) between retry attempts -->
                <Attribute name='lockwaitThreshold' value='5000'/>
              </Object>
            </Attribute>
            <!--controls optimization in UserView that allows AuditPolicy to
                be executed in the callers thread, rather than in a Task
              -->
            <Attribute name='user'>
               <Object>
                  <Attribute name='view'>
                     <Object>
                        <Attribute name='alwaysUseTask'>
                           <Boolean>false</Boolean>
                        </Attribute>
                     </Object>
                  </Attribute>
               </Object>
            </Attribute>
            <Attribute name='debugger'>
               <Object>
                   <Attribute name='enabled'>
                      <Boolean>true</Boolean>
                   </Attribute>
               </Object>
            </Attribute>
            <Attribute name='scheduler'>
              <Object>
                <!-- Indicates whether the scheduler should be started
                     at server startup.  Valid values are "enabled",
                     "manual", and "disabled".  Other values are
                     interpreted as "enabled", excepting "suspended"
                     (which is a synonym for "manual").
                     If "disabled", the Scheduler CANNOT be started on
                     the server, even through the API.  If "manual",
                     the Scheduler needs to be started through the API.
                     NOTE: For backward compatibility, the
                     task.scheduler.enabled and task.scheduler.suspended
                     properties from Waveset.properties will override
                     this setting.
                -->
                <Attribute name='startup' value='enabled'/>
                <!-- Enables trace logging to stdout -->
                <Attribute name='trace' value='false'/>
                <!-- Disables processing of scheduled tasks -->
                <Attribute name='disableScheduled' value='false'/>
                <!-- Enables tracing of task result cleanup -->
                <Attribute name='traceTaskResultCleanup' value='false'/>
                <!-- Disables removal of ophaned task results -->
                <Attribute name='disableTaskResultCleanup' value='false'/>
                <!-- Enables periodic removal of orphaned task results.
                     If disabled, this clean up is only done at startup.
                  -->
                <Attribute name='scheduledTaskResultCleanup' value='false'/>
                <!-- Lists task definitions (by name or id) which are
                     subject to task restrictions.
                -->
                <Attribute name='tasklist'>
                  <List/>
                </Attribute>
                <!-- Indicates whether the tasklist for task restrictions
                     enumerates the tasks which ARE NOT allowed to run
                     ("exclude") or all of the tasks which ARE allowed
                     to run ("restrict").  Other values are interpreted as
                     "exclude".
                 -->
                <Attribute name='tasklistMode' value='exclude'/>
              </Object>
            </Attribute>
            <!-- Default settings for the email template smtpHost value.
            -->
            <Attribute name='emailtemplate'>
              <Object>
                  <Attribute name='smtpHost' value='mail.example.com'/>
				  <Attribute name='port' value='25'/>
				  <Attribute name='authEnabled' value='false'/>
				  <Attribute name='ssl' value='false'/>
				  <Attribute name='ignoreCert' value='false'/>
              </Object>
            </Attribute>
            <!-- Default settings for JXM
            -->
            <Attribute name='jmx'>
              <Object>
                <!-- default to disable -->
                <Attribute name='enable' value='false'/>
                <!-- default to 10min -->
                <Attribute name='pollinterval' value='60000'/>
              </Object>
            </Attribute>
          </Object>
        </Attribute>
      </Object>
    </Attribute>

