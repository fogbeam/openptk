                <String>com.sun.idm.logging.audit.JMXPublisher</String>
                <String>com.sun.idm.logging.audit.JMSPublisher</String>
                <String>com.sun.idm.logging.audit.ScriptedPublisher</String>
              </List>
            </Attribute>
          </Object>
        </Attribute>
		
