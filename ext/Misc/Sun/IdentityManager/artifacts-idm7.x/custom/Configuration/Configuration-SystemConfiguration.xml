      <!-- Workflow Options -->
      <Attribute name='workflow'>
         <Object>
            <!-- Specifies the level of detail included in workflow  -->
            <!-- trace. An unspecified or 0 value generates the most -->
            <!-- detail.                                             -->
            <Attribute name='traceLevel' value='1'/>

            <!-- Turn on tracing of generic objects.                 -->
            <Attribute name='traceAllObjects' value='false'/>

            <!-- When true, trace messages will be accumulated in the -->
            <!-- task's result object.                                -->
            <Attribute name='resultTrace' value='false'/>

            <!-- When true, trace messages will be emitted to the console. -->
            <Attribute name='consoleTrace' value='false'/>

            <!-- When set, trace messages will be emitted to the named -->
            <!-- file.                                                 -->
            <Attribute name='fileTrace' value=''/>

            <!-- When true, we will return the entire case history in the   -->
            <!-- TaskInstance after the task has completed.  Normally we    -->
            <!-- prune out the case history to save space since it can be   -->
            <!-- quite large.  It is however occasionally useful to examine -->
            <!-- the history to diagnose process problems.                  -->
            <Attribute name='retainHistory' value='false'/>

            <!-- Max steps before we terminate a workflow                   -->
            <Attribute name='maxSteps' value='5000'/>

            <!-- Level of strictness applied when validating workflows      -->
            <!-- prior to running them. Errors of this level or greater     -->
            <!-- will result in the workflow not being run.                 -->
            <!-- Valid values are CRITICAL, ERROR, WARNING, or NONE,        -->
            <!-- where NONE turns off validation completely.                -->
            <Attribute name='validationLevel' value='CRITICAL'/>
         </Object>
      </Attribute>

      <!-- Setting this to true exposes links to the scripts used to
           generate the graphs.  This is useful if a problem is encountered -->
      <Attribute name='dashboard'>
        <Object>
          <Attribute name='debug' value='false'/>
        </Object>
      </Attribute>

      <!-- Settings to control how email notifications are sent. -->
      <Attribute name="emailNotifier">
        <Object>
          <!-- If set to true, then localized emails will be sent when
               the subject and body of the email messages are message
               catalog keys. -->
          <Attribute name='enableLocalization' value='true'/>

          <!-- The character set to use when sending emails.  
               If none is set, then UTF-8 is used. -->
           <Attribute name='contentCharset' value='UTF-8'/>
        </Object>
      </Attribute>

            <!-- Processing flag tells Deferred Task Scanner   -->
            <!-- that it need only query for deferred tasks.   -->
            <Attribute name='2005Q4M3refreshOfTypeUserIsComplete'>
                <Boolean>true</Boolean>
            </Attribute>
	
